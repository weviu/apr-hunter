# API Keys Configuration
# Copy this file to .env.secrets and fill in your actual API keys
# DO NOT commit .env.secrets to version control!

# Binance Earn API
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_API_SECRET=your_binance_api_secret_here

# OKX API
OKX_API_KEY=your_okx_api_key_here
OKX_API_SECRET=your_okx_api_secret_here
OKX_PASSPHRASE=your_okx_passphrase_here

# KuCoin API
KUCOIN_API_KEY=your_kucoin_api_key_here
KUCOIN_API_SECRET=your_kucoin_api_secret_here
KUCOIN_PASSPHRASE=your_kucoin_passphrase_here

# Alert Evaluation Secret
ALERT_EVAL_SECRET=your_alert_eval_secret_here

# Enable live exchange data fetching (Binance, OKX, KuCoin)
# Set to 'true' to fetch real APR data from exchanges
# Set to 'false' or omit to use sample data
ENABLE_LIVE_EXCHANGE_FETCH=true
